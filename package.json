{
  "name": "angularjs-bootstrap-jasmine",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/ashawley/angularjs-bootstrap-jasmine",
  "dependencies": {
    "angular": "^1.6.8",
    "angular-loader": "^1.6.8",
    "angular-route": "^1.6.8",
    "angular-sanitize": "^1.6.8",
    "angular-ui-bootstrap": "^2.5.6",
    "bootstrap": "3.3.7",
    "jquery": "2.1.4"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.8",
    "beefy": "^2.1.8",
    "browserify": "^15.1.0",
    "browserify-shim": "^3.8.14",
    "budo": "^10.0.4",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.1.3",
    "jasmine-jquery": "^2.1.1",
    "jshint": "^2.9.5",
    "karma": "^0.13.22",
    "karma-browserify": "^5.1.3",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.4",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.2.3",
    "karma-junit-reporter": "^0.4.2",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.3",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.16",
    "shelljs": "^0.7.8",
    "uglify-js": "^3.3.5",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "scripts": {
    "prestart": "npm install && npm run build",
    "postinstall": "npm run copy-libs",
    "copy-libs": "node scripts/copy-libs.js",
    "lint": "jshint ./app --exclude ./app/dist",
    "prebuild": "npm run lint",
    "build:browserify": "browserify ./app/index.js -o ./app/dist/index.js",
    "build:uglifyjs": "uglifyjs ./app/dist/index.js -c -o ./app/dist/index.min.js",
    "build:less": "lessc --clean-css ./app/style/index.less ./app/dist/index.css",
    "build": "npm run build:browserify && npm run build:uglifyjs && npm run build:less",
    "watch:browserify": "watchify ./app/index.js -d -o ./app/dist/index.js",
    "watch:less": "watch 'npm run build:less' ./app/style",
    "watch:build": "npm run watch:less & npm run watch:browserify",
    "pretest": "npm install && npm run build",
    "test": "karma start --single-run",
    "watch:test": "karma start --auto-watch",
    "start": "http-server -a localhost -p 8080 -c-1 ./app",
    "start:beefy": "beefy app/index.js:dist/index.js 8080 --index=./app/index.html --cwd ./app --open --live -- -o app/dist/index.js",
    "start:budo": "budo app/index.js:dist/index.js -v -H localhost -p 8080 -d ./app --open --live -- -o app/dist/index.js",
    "watch:start": "npm run start:budo"
  }
}
